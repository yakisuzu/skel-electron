{
  "name": "skel-electron",
  "version": "1.0.0",
  "description": "text",
  "readme": "readme.md",
  "main": "build.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yakisuzu/skel-electron.git"
  },
  "bugs": {
    "url": "https://github.com/yakisuzu/skel-electron/issues"
  },
  "homepage": "https://github.com/yakisuzu/skel-electron#readme",
  "keywords": [],
  "scripts": {
    "electron": "electron .",
    "eslint": "eslint src/",
    "babel": "babel src/ --out-dir app/",
    "babel-watch": "babel src/ --out-dir app/ --watch",
    "pkg-mac": "node build.js mac",
    "pkg-win32": "node build.js win32"
  },
  "author": "yakisuzu",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-es2015-template-literals": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "electron": "^1.4.14",
    "electron-packager": "^8.5.1",
    "eslint": "^3.13.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-class-properties",
      "transform-es2015-template-literals",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "rules": {
      "no-console": "warn"
    }
  }
}
