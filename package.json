{
  "name": "skel-electron",
  "version": "1.0.0",
  "description": "text",
  "readme": "readme.md",
  "main": "dest/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yakisuzu/skel-electron.git"
  },
  "scripts": {
    "eslint": "eslint src/",
    "babel": "babel src/ --out-dir dest/",
    "babel-watch": "babel src/ --out-dir dest/ --watch"
  },
  "author": "yakisuzu",
  "license": "MIT",
  "devDependencies": {
    "asar": "^0.13.0",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "electron": "^1.4.14",
    "eslint": "^3.13.1",
    "react": "^15.4.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "rules": {
      "no-console": "warn"
    }
  }
}
